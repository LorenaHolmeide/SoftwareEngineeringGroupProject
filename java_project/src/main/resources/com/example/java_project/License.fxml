<?xml version="1.0" encoding="UTF-8"?>

<?<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.java_project.LicenseController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <ImageView fitHeight="38.0" fitWidth="60.0" layoutX="6.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@car.png" />
            </image>
        </ImageView>
        <Label layoutX="70.0" layoutY="9.0" prefHeight="19.0" prefWidth="201.0" text="Profile Details">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="23.0" />
            </font>
        </Label>
        <Label layoutX="104.0" layoutY="58.0" text="First name">
            <font>
                <Font name="Comic Sans MS Bold" size="13.0" />
            </font>
        </Label>
        <Label layoutX="97.0" layoutY="97.0" prefHeight="19.0" prefWidth="74.0" text="Last name">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="13.0" />
            </font>
        </Label>
        <Label layoutX="95.0" layoutY="266.0" text="Post number">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="13.0" />
            </font>
        </Label>

        <TextField fx:id="tf_drivingLicense" layoutX="107.0" layoutY="295.0" prefHeight="25.0" prefWidth="149.0" promptText="Driving licensed number" />
        <Label fx:id="UpdatePrompt" layoutX="315.0" layoutY="20.0" textFill="#f81212">
            <font>
                <Font name="Comic Sans MS" size="13.0" />
            </font>
        </Label>
        <TextField fx:id="dateTxtField" layoutX="260.0" layoutY="295.0" prefHeight="25.0" prefWidth="110.0" promptText="Date of Issue" />
        <TextField fx:id="countryTxtField" layoutX="393.0" layoutY="295.0" prefHeight="25.0" prefWidth="119.0" promptText="Country of Issue" />
        <Button layoutX="527.0" layoutY="295.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="45.0" text="Add" />
    </children>

</AnchorPane>
