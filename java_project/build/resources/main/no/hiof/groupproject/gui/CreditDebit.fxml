<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="scenePane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #9fc5e8;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="no.hiof.groupproject.gui.CreditDebitController">
    <children>
        <Label layoutX="25.0" layoutY="26.0" text="Payment Page">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="15.0" />
            </font>
        </Label>
        <Label layoutX="96.0" layoutY="96.0" text="card number">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="16.0" />
            </font>
        </Label>
        <Label layoutX="96.0" layoutY="152.0" text="CCV">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="14.0" />
            </font>
        </Label>
        <Button fx:id="button_addPayment" layoutX="373.0" layoutY="297.0" mnemonicParsing="false" onAction="#btn_AddPayment" prefHeight="27.0" prefWidth="123.0" style="-fx-background-color: #f1c232;" text="Add Payment">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="15.0" />
            </font>
        </Button>
        <TextField fx:id="tf_EmailAdd" layoutX="226.0" layoutY="92.0" prefHeight="25.0" prefWidth="182.0" promptText="Enter phone number">
