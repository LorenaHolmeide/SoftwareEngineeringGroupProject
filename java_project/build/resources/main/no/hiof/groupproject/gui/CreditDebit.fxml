      <Label layoutX="98.0" layoutY="215.0" text="Valid until">
         <font>
            <Font name="Comic Sans MS Bold" size="14.0" />
         </font>
      </Label>

      <Label fx:id="emptyFieldWarning" layoutX="391.0" layoutY="43.0" prefHeight="32.0" prefWidth="146.0" />
      <Button fx:id="backButton" layoutX="397.0" layoutY="36.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="182.0" style="-fx-background-color: #9fc5e8;" text="Return to previous page">
